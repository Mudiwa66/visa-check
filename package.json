{
  "name": "visa-checker",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "start:clear": "expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "clean:cache": "rm -rf .expo && rm -rf node_modules/.cache && watchman watch-del-all 2>/dev/null; echo 'Cache cleared'",
    "clean:metro": "rm -rf $TMPDIR/metro-* $TMPDIR/haste-map-* 2>/dev/null; echo 'Metro cache cleared'",
    "clean:npm": "npm cache clean --force && echo 'npm cache cleared'",
    "clean:modules": "rm -rf node_modules && rm -f package-lock.json && echo 'node_modules and lock file removed'",
    "clean:full": "npm run clean:cache && npm run clean:metro && npm run clean:modules && npm run clean:npm && echo 'Full clean complete'",
    "reinstall": "npm run clean:full && npm install",
    "nuke": "npm run clean:full && npm install && npm run start:clear",
    "preinstall": "node -e \"const os=require('os');const free=os.freemem()/os.totalmem()*100;if(free<20)console.warn('\\x1b[33m⚠️  Warning: Low memory (' + free.toFixed(1) + '% free). Consider closing apps.\\x1b[0m');\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "debug": "^4.4.3",
    "expo": "~54.0.31",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.16",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "jest-watch-typeahead": {
      "jest": "^30.2.0"
    },
    "react-test-renderer": "19.1.0"
  },
  "private": true
}
